  "description": "new york tamd cooking",
  "permissions": ["activeTab", "declarativeContent","storage"],
  "background": {
    "scripts": ["./index.bs.js"],
    "persistent": false
  },
  "page_action": {
    "default_popup": "./index.html",
    "default_icon": {
      "16": "assets/16.png",
